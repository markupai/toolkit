{
  "name": "@markupai/toolkit",
  "version": "0.2.1",
  "homepage": "https://github.com/markupai/toolkit-ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:markupai/toolkit-ts.git"
  },
  "license": "Apache-2.0",
  "author": "Markup AI",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/toolkit.js",
      "require": "./dist/toolkit.umd.cjs"
    }
  },
  "main": "./dist/toolkit.umd.cjs",
  "module": "./dist/toolkit.js",
  "types": "./dist/types/main.d.ts",
  "files": [
    "dist",
    "dist/types/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "vitest -c vitest.integration.config.ts",
    "test:unit": "vitest run -c vitest.unit.config.ts"
  },
  "dependencies": {
    "@markupai/api": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.4.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "msw": "^2.11.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
